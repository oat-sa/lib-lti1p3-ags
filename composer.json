{
    "name": "oat-sa/lib-lti1p3-ags",
    "description": "OAT LTI 1.3 Advantage AGS Library",
    "type": "library",
    "license": "GPL-2.0-only",
    "require": {
        "php": ">=8.0.0",
        "ext-json": "*",
        "nesbot/carbon": "^2.72 || ^3.0",
        "nyholm/psr7": "^1.8",
        "oat-sa/lib-lti1p3-core": "7.2.2",
        "psr/http-message": "^1.1 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^12",
        "psalm/plugin-phpunit": "^0.19",
        "vimeo/psalm": "^4.6 || ^5.25 || ^6.8"
    },
    "autoload": {
        "psr-4": {
            "OAT\\Library\\Lti1p3Ags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OAT\\Library\\Lti1p3Ags\\Tests\\": "tests/",
            "OAT\\Library\\Lti1p3Core\\Tests\\": "vendor/oat-sa/lib-lti1p3-core/tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
